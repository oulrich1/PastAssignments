# GNU Make solution makefile autogenerated by Premake
# Type "make help" for usage help

ifndef config
  config=release
endif
export config

PROJECTS := Lib Maze

.PHONY: all clean help $(PROJECTS)

all: $(PROJECTS)

Lib: 
	@echo "==== Building Lib ($(config)) ===="
	@${MAKE} --no-print-directory -C build/common-lib -f Makefile

Maze: Lib
	@echo "==== Building Maze ($(config)) ===="
	@${MAKE} --no-print-directory -C build/maze -f Makefile

clean:
	@${MAKE} --no-print-directory -C build/common-lib -f Makefile clean
	@${MAKE} --no-print-directory -C build/maze -f Makefile clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "   release"
	@echo "   debug"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   Lib"
	@echo "   Maze"
	@echo ""
	@echo "For more information, see http://industriousone.com/premake/quick-start"
